{
  "name": "worldland-monorepo",
  "private": true,
  "workspaces": [
    "landing",
    "cloud-console",
    "docs"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:landing\" \"npm run dev:cloud\" \"npm run dev:docs\"",
    "dev:landing": "npm run dev --workspace=landing -- -p 3000",
    "dev:cloud": "npm run dev --workspace=liber-cli -- -p 3001",
    "dev:docs": "npm run dev --workspace=worldland-docs -- --port 3002",
    "build": "npm run build --workspaces",
    "lint": "npm run lint --workspaces"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
